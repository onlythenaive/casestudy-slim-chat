{{#partial "content"}}

    <h1>My contacts</h1>

    <ul>
        {{#data.contacts}}
            <li>
                <span>
                    <img src="{{gravatarUrl}}">
                </span>
                <span>
                    <a href="profiles/{{id}}">{{prettyName}}</a>
                </span>
            </li>
        {{/data.contacts}}
    </ul>
{{/partial}}

{{> plain/templates/layout}}
